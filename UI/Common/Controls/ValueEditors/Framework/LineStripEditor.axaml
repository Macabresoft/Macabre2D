<valueEditors:ValueEditorControl xmlns="https://github.com/avaloniaui"
                                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                 xmlns:local="clr-namespace:Macabresoft.Macabre2D.UI.Common"
                                 xmlns:framework="clr-namespace:Macabresoft.Macabre2D.Framework;assembly=Macabre2D.Framework"
                                 xmlns:valueEditors="clr-namespace:Macabresoft.Macabre2D.UI.Common"
                                 x:TypeArguments="framework:LineStrip"
                                 x:Class="Macabresoft.Macabre2D.UI.Common.LineStripEditor">
    <Grid DataContext="{Binding $parent[local:LineStripEditor]}">
        <Design.DataContext>
            <local:LineStripEditor />
        </Design.DataContext>
        <TextBlock>Line Strip Editor Under Construction</TextBlock>
    </Grid>
</valueEditors:ValueEditorControl>